{
  "name": "centurion-framework",
  "projectName": "Centurion Framework",
  "version": "3.8.1",
  "simpleDescription": "A Web Framework",
  "description": "Centurion is a web-based framework for rapid prototyping and provides a core for building larger web projects",
  "homepage": "http://www.centurionframework.com",
  "author": {
    "name": "Justin Hough",
    "email": "<justin@justinhough.com>",
    "url": "http://www.justinhough.com/"
  },
  "contributors": [],
  "license": "GPL",
  "repository": "justinhough/centurion",
  "bugs": "https://github.com/justinhough/centurion/issues",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-series sass styleguide",
    "styleguide": "nucleus --config config.nucleus.json",
    "prerelease": "npm-run-series changelog",
    "sass": "node-sass -r ./lib/sass/ -o ./dist/css/",
    "release": "gh-release",
    "prestart": "npm-run-series sass",
    "start": "parallelshell 'rerun-script'"
  },
  "watches": {
    "test": "echo \"Watch test specified\" && exit 1"
  },
  "pre-commit": [
    "test"
  ],
  "keywords": [
    "centurion",
    "framework",
    "responsive",
    "css",
    "sass",
    "builder",
    "prototyping"
  ],
